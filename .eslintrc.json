{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-prototype-builtins": 0,
        "no-console": 1,
        "no-unused-vars": 1,
        "no-undef": 1
    },
    "globals": {
        "wpu_pinned_comments_options": true,
        "wpu_pinned_comments_admin": true,
        "wp": true,
        "jQuery": true
    }
}
